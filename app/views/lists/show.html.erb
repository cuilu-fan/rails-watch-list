<div><%= @list.name %></div>
<div><%= link_to "edit list name", edit_list_path(@list) %></div>
<%= simple_form_for [@list, @bookmark] do |f| %>
<%= f.association :movie  %>
<%= f.input :comment %>
<%= f.button :submit, class: "btn btn-primary"  %>
<% end %>
<div>
  <% @movies.each do |movie| %>
  <div>
    <%= movie.title %>
    <%= image_tag movie.poster_url %>
    <%= movie.rating %>
    <%= movie.overview %>
  </div>
  <% end %>
</div>
